<manifest xmlns:ml="magicleap"
          ml:package="com.magicscript.sample.exclusive"
          ml:version_name="0.0.1"
          ml:version_code="1">
  <application ml:visible_name="Exclusive Sample"
               ml:application_type="system"
               ml:sdk_version="1.0">
    <component ml:name=".fullscreen"
               ml:visible_name="Exclusive Sample"
               ml:binary_name="bin/index.js"
               ml:type="Fullscreen">
      <mode ml:shareable="true"/>
      <icon ml:model_folder=""
            ml:portal_folder=""/>
    </component>
    <uses-privilege ml:name="CopresenceAccess"/>
    <uses-privilege ml:name="VolumeRelativeVolume"/>
    <uses-privilege ml:name="LowLatencyLightwear"/>
    <uses-privilege ml:name="UniverseExclusiveMode"/>
    <uses-privilege ml:name="AppVisibility"/>
    <uses-privilege ml:name="MagicScript"/>
  </application>
</manifest>
